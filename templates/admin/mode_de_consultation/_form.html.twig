
<div class="col-sm-12 col-xl-12">
    <div class="bg-light rounded h-100 p-4">
        <h6 class="mb-4"><a href="{{ path('app_admin_mode_de_consultation_index') }}">Retournée a la liste</a></h6>
        {{ form_start(form) }}
        <div class="row mb-3">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Titre</label>
            <div class="col-sm-10">
                {{ form_widget(form.Titre) }}
            </div>
        </div>
        <div class="row mb-3">
            <label for="product_PriceHT" class="col-sm-2 col-form-label">Description page d'accueil</label>
            <div class="col-sm-10">
                {{ form_widget(form.Description) }}
            </div>
        </div>
        <div class="row mb-3">
            <label for="product_PriceTTC" class="col-sm-2 col-form-label">Page métier</label>
            <div class="col-sm-10">
                {{ form_widget(form.GrandeDescription) }}
            </div>
        </div>
        <div class="row mb-3">
            <label for="product_PriceTTC" class="col-sm-2 col-form-label">Image page métier</label>
            <div class="col-sm-10">
                {{ form_widget(form.imageFile) }}
            </div>
        </div>

        <button type="submit" class="btn btn-primary">{{ button_label|default('Enregistré') }}</button>
        {{ form_end(form) }}
    </div>
</div>

 