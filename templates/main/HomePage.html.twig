{% extends 'base.html.twig' %}

{% block title %}Voyance en ligne : Consultation de voyants expérimentés - Ursuliah!
{% endblock %}
{% block description %}Obtenez des réponses à vos questions avec nos voyants expérimentés en ligne. Consultez dès maintenant pour des prédictions précises et fiables.
{% endblock %}

{% block body %}
	<div class="th-hero-wrapper hero-3" id="hero">
		<div class="hero-slider-2 th-carousel" data-fade="true" data-slide-show="1" data-md-slide-show="1" data-arrows="true">

			{% for slider in sliders %}

				<div class="th-hero-slide">
					<div class="th-hero-bg" data-bg-src="{{ vich_uploader_asset(slider, 'imageFile') }}" data-overlay="black" data-opacity="7"></div>
					<div class="container">
						<div class="row align-items-center justify-content-center">
							<div class="col-lg-9">
								<div class="hero-style3 text-center">
									<span class="hero-subtitle text-theme" data-ani="slideinup" data-ani-delay="0.1s">{{ slider.Description }}</span>
									<h1 class="hero-title text-white" data-ani="slideinup" data-ani-delay="0.4s">{{ slider.Title }}</h1>
									<div class="btn-group justify-content-center" data-ani="slideinup" data-ani-delay="0.7s">
										<a href="/{{ slider.Slug }}" class="th-btn style3">{{ slider.NameSlug }}<i class="fa-regular fa-arrow-right ms-3"></i>
										</a>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>

			{% endfor %}

		</div>
	</div>

	<section class="">
		<div class="container">
			<div class="row slider-shadow th-carousel service-slider3" data-slide-show="6" data-lg-slide-show="5" data-md-slide-show="2" data-sm-slide-show="1" data-xs-slide-show="1">

				{% for horoscope in horoscopes %}

					<div class="col-md-6 col-lg-4">
						<div class="service-box" style="height:200px">
							<div class="service-bg-shape">
								<img src="{{ asset('assets/img/service/service_box_shape1.jpg') }}" alt="img">
							</div>
							<div class="service-box_content">
								<div class="service-default-icon {{horoscope}}">
									<img src="{{ asset('assets/img/horoscope/svg/' ~ horoscope ~ '.svg') }}" height="50" width="50" alt="Icon">
								</div>
								<h3 class="box-title">
									<a href="{{ path('app_horoscope', {'slug': horoscope}) }}">{{ horoscope | upper }}</a>
								</h3>
							</div>
						</div>
					</div>

				{% endfor %}

			</div>
		</div>
	</section>

	<div class="overflow-hidden space" data-bg-src="{{ asset('assets/img/bg/about-bg-3.png') }}">
		<div class="container">
			<div class="row align-items-center">
				<div class="col-xl-7 mb-30 mb-xl-0 order-xl-2">
					<div class="img-box3">
						<img class="img-1 tilt-active" src="{{ asset('assets/img/normal/about_3_1.png') }}" alt="About">
						<img class="img-2 tilt-active" src="{{ asset('assets/img/normal/about_3_2.png') }}" alt="About">
						<a href="https://www.youtube.com/watch?v=_sI_Ps7JSEk" class="play-btn popup-video style5" tabindex="0">
							<i class="fas fa-play"></i>
						</a>
					</div>
				</div>

				<div class="col-xl-5 order-xl-1">
					<div class="title-area mb-35">
						<span class="sub-title mb-3">Qui suis-je</span>
						<div class="mb-3">
							Xavier Médium:
                                    <i class="fa-solid fa-star-sharp" {% if moyenne > 0 %} style="color:yellow" {% endif %}></i>
                                    <i class="fa-solid fa-star-sharp" {% if moyenne > 1 %} style="color:yellow" {% endif %}></i>
                                    <i class="fa-solid fa-star-sharp" {% if moyenne > 2 %} style="color:yellow" {% endif %}></i>
                                    <i class="fa-solid fa-star-sharp" {% if moyenne > 3 %} style="color:yellow" {% endif %}></i>
                                    <i class="fa-solid fa-star-sharp" {% if moyenne > 4 %} style="color:yellow" {% endif %}></i>
						</div>
						<h6>Astologue, Cartomancien, Voyant & médium, Numérologue, Tarologue</h6>
						<p>lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi.</p>
					</div>
					<div id="btn-home" class="btn-group" style="gap: 29px"></div>
				</div>
			</div>
		</div>
	</div>

	<section class="service-area3-2 space-top" data-bg-src="assets/img/bg/service3-bg.png">
		<div class="container">
			<div class="title-area text-center">
				<span class="sub-title">Voyance en ligne avec Xarox</span>
				<h2 class="sec-title text-white">Explorez votre destinée en ligne aux côtés de Xarox</h2>
			</div>
			<div class="row th-carousel" data-slide-show="3" data-lg-slide-show="3" data-md-slide-show="2" data-sm-slide-show="1" data-xs-slide-show="1">

				{% for consultation in modeDeConsultations %}

					<div class="col-md-6 col-lg-4">
						<div class="service-box style2">
							<div class="service-box_content">
								<div class="service-default-icon">
									<img src="{{ asset('assets/img/service/' ~ consultation.Slug ~ '.svg') }}" height="30" width="30" alt="img">
								</div>
								<h3 class="box-title">
									<a href="{{ path('app_voyance_immediate', {'Slug': consultation.Slug}) }}">{{ consultation.Titre }}</a>
								</h3>
								<p class="service-box_text">{{ consultation.Description }}</p>
							</div>
							<div class="thumb">
								<img src="{{ asset('assets/img/service/' ~ consultation.Slug ~ '.png') }}" alt="img">
								<a href="{{ path('app_voyance_immediate', {'Slug': consultation.Slug}) }}" class="icon">
									<i class="fas fa-arrow-right"></i>
								</a>
							</div>
						</div>
					</div>

				{% endfor %}

			</div>
		</div>
	</section>
	<div class="container">
		<div class="cta-area-1" data-bg-src="{{ asset('assets/img/bg/cta-bg1.png') }}" data-overlay="theme" data-opacity="9">
			<div class="row align-items-center justify-content-between">
				<div class="col-lg-auto">
					<div class="title-area mb-lg-0 text-lg-start text-center">
						<h2 class="sec-title text-white mb-0">Nos forfaits prépayer</h2>
					</div>
				</div>
				<div class="col-lg-auto text-center">
					<a href="{{ path('user_forfait') }}" class="th-btn style2">Acheter des minutes<i class="fa-regular fa-arrow-right ms-2"></i>
					</a>
				</div>
			</div>
		</div>
	</div>


	<div class="experience-area-1 space" data-bg-src="{{ asset('assets/img/bg/experience-bg-1.png') }}">
		<div class="experience-bg-thumb shape-mockup" data-right="0" data-top="93px">
			<img src="assets/img/normal/experience-bg-shape.png" alt="img">
		</div>
		<div class="container">
			<div class="row align-items-center">
				<div class="col-xl-5 order-xl-1">
					<div class="title-area mb-35">
						<span class="sub-title">Qui suis-je</span>
						<h2 class="sec-title">Guide vers la voyance et l'épanouissement personnel</h2>
					</div>
					<p class="mt-n2 mb-30">Médium, tarologue, numérologue, astrologue. Pour une voyance, claire et précise. Pour que nous puissions trouver ensemble des solutions à  vos problèmes sentimentaux et professionnels.</p>
					<div class="btn-group">
						<a href="{{ path('app_aboutus') }}" class="th-btn">En savoir plus<i class="fa-regular fa-arrow-right ms-2"></i>
						</a>

					</div>

				</div>
				<div class="col-xl-6">
					<div class="title-area mb-35">
						<span class="sub-title">Sattidfaction des clients</span>
						<h2 class="sec-title">Avis Clients de xaros</h2>
					</div>
					<div class="skill-feature">
						<h5 class="skill-feature_title">Tres satisfait</h5>
						<div class="progress">
							<div class="progress-bar" style="width: {{moyenneAvis.tresSatisfait}}%;">
								<div class="progress-value">{{moyenneAvis.tresSatisfait}}%</div>
							</div>
						</div>
					</div>
					<div class="skill-feature mt-30">
						<h5 class="skill-feature_title">satisfait</h5>
						<div class="progress">
							<div class="progress-bar" style="width: {{moyenneAvis.pasSatisfait}}%;">
								<div class="progress-value">{{moyenneAvis.pasSatisfait}}%</div>
							</div>
						</div>
					</div>
					<div class="skill-feature mt-30">
						<h5 class="skill-feature_title">peut satisfait</h5>
						<div class="progress">
							<div class="progress-bar" style="width: {{moyenneAvis.pasDuToutSatisfait}}%;">
								<div class="progress-value">{{moyenneAvis.pasDuToutSatisfait}}%</div>
							</div>
						</div>
					</div>
					<div class="btn-wrap mt-50">
						<a href="{{ path('app_avis') }}" class="th-btn">Voir les avis clients<i class="fa-regular fa-arrow-right ms-2"></i>
						</a>
					</div>
				</div>
			</div>
		</div>
	</div>

	{# integration avec Mailshimp #}
	<section class="newsletter-area bg-theme" data-bg-src="{{ asset('assets/img/bg/newsletter-bg.png') }}">
		<div class="newsletter-thumb shape-mockup d-lg-block d-none" data-top="0" data-left="0"><img src="assets/img/bg/newsletter-bg1.png" alt="img"></div>
		<div class="container">
			<div class="row justify-content-end align-items-center">
				<div class="col-lg-5 col-md-8">
					<div class="title-area mb-0 me-xl-5 text-md-start text-center">
						<h2 class="sec-title h3 text-white">Inscrivez-vous à notre newsletter pour ne rien rater</h2>
					</div>
				</div>
				<div class="col-lg-4 col-md-4 mt-4 mt-md-0 text-md-end">
					<div class="widget newsletter-widget footer-widget mb-0">
						<form class="newsletter-form">
							<input class="form-control" type="email" placeholder="Email Address" required="">
							<button type="submit" class="th-btn">
								<i class="far fa-paper-plane"></i>
							</button>
						</form>
					</div>
				</div>
			</div>
		</div>
	</section>
{% endblock %}
